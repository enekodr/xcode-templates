<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.IDEFoundation.TextSubstitutionFileTemplateKind</string>
	<key>Description</key>
	<string>A sweet group of files creating a nice and clean VIPER module.</string>
	<key>Summary</key>
	<string>VIPER architecture module.</string>
	<key>SortOrder</key>
	<string>30</string>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>DefaultCompletionName</key>
	<string></string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.swift</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>className</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>Base class name:</string>
			<key>Description</key>
			<string>The base class name to be reused in all classes of the module.</string>
			<key>Type</key>
			<string>text</string>
			<key>NotPersisted</key>
			<false/>
		</dict>
	</array>
</dict>
</plist>
